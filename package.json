    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^3.0.2",
    "urijs": "^1.19.9"
  },
  "scripts": {
    "prepare": "npm run clean && tsc -p tsconfig.json -outDir . && npm run copy",
    "tests": "jasmine --config=jasmine.json",
    "copy": "copyfiles -u 1 src/**/*.scss . && shx cp -R src/icons .",
    "dev": "npm run prepare",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "clean": "shx rm -rf src/**/*.d.ts src/**/*.js src/**/*.js.map *.js *.js.map *.d.ts *.scss && node build/rmrf.js"
  },
  "devDependencies": {
    "@blueprintjs/core": "^2.3.1",
