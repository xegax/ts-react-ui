{
  "name": "ts-react-ui",
  "version": "0.1.42",
  "dependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/draft-js": "^0.10.44",
    "draft-js": "^0.11.7",
    "font-awesome": "^4.7.0",
    "jasmine": "^3.5.0",
    "node-sass": "^4.14.1",
    "objio": "^0.0.42",
    "react": "^16.4.1",
    "react-color": "^2.17.3",
    "react-dom": "^16.4.1",
    "react-tooltip": "^4.1.2",
